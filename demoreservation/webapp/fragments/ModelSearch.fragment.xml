<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Models"
		class="sapUiPopupWithPadding"
		items="{DemoOdataModel>/ZC_MODEL_YEAR}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		<StandardListItem
			title="{DemoOdataModel>model_desc_en}"
			description="{DemoOdataModel>model}" />
	</SelectDialog>
</core:FragmentDefinition>