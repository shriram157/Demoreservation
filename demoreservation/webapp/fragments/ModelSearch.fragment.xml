<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		title="Models"
		class="sapUiPopupWithPadding"
		items="{/ZC_MODEL_YEAR}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		<StandardListItem
			title="{model_desc_en}"
			description="{model}" />
	</SelectDialog>
</core:FragmentDefinition>