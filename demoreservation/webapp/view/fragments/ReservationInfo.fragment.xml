<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout" xmlns:uxap="sap.uxap"
	xmlns:form="sap.ui.layout.form">
	<Dialog title="Reservation Info" type="Message" contentHeight="60%" contentWidth="60%" id="feedbackDialog">
		<content>
			<Text text="Vehicle Info" class="sapUiSmallMargin font_size_larger"/>
			<HBox>
				<VBox width="50%">
					<items>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Description"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/MATNR}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Model year"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/ZZMOYR}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Colour"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/Colour}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Driver"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/Driver}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Start Date"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/EstStartDt}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Est. Assoc Price"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
							<Text text="{VehicleInfo>/EstAssPrice}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Status"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/Status}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="SFX"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/ZZSUFFIX}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Zone"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/ZZZONE}"/>
							</items>
						</HBox>
					</items>
				</VBox>
				<VBox width="50%">
					<items>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Series"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/ZZSERIES}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="VIN"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/VHVIN}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Colour Code"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/ZZEXTCOL}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Driver Dept"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/QAUSP}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Est. End Date"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/EndDate}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Est. Non Assoc. Price"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/NonAssPrice}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="SFX Options"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/Option}"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="APX"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Text text="{VehicleInfo>/ZZAPX}"/>
							</items>
						</HBox>

					</items>
				</VBox>
			</HBox>
			<Text text="Reservation Details" class="sapUiSmallMargin font_size_larger"/>
			<HBox>
				<VBox width="50%">
					<Text text="Requestor Information" class="sapUiTinyMargin"/>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Requestor Type"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Text text="{VehicleInfo>/ZZREQTYP}"/>
						</items>
					</HBox>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Requestor Name"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Text text="{VehicleInfo>/RequestorName}"/>
						</items>
					</HBox>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Request Date"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Text text="{VehicleInfo>/zzcretms}"/>
						</items>
					</HBox>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Requests Approved"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Text text="{VehicleInfo>/ReqAppr}"/>
						</items>
					</HBox>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Requests Pending Approval"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Text text="{VehicleInfo>/ReqPend}"/>
						</items>
					</HBox>
				</VBox>
				<VBox width="50%">
					<Text text="Reserved For Information" class="sapUiTinyMargin"/>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Reserved for Type"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Text text="{VehicleInfo>/ZZPURTYP}"/>
						</items>
					</HBox>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Reserved For Name"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Text text="{VehicleInfo>/ZZPURNAME}"/>
						</items>
					</HBox>
				</VBox>
			</HBox>
		</content>
		<beginButton>
			<Button press="onApprovePress" text="Approve"/>
		</beginButton>
		<endButton>
			<Button press="onCloseDialog" text="Cancel"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>