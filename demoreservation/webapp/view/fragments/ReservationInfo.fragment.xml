<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout" xmlns:uxap="sap.uxap" xmlns:form="sap.ui.layout.form">
	<Dialog title="Reservation Info" type="Message" contentHeight="60%" contentWidth="60%" id="feedbackDialog">
		<content>
			<Label text="Vehicle Info" class="sapUiSmallMargin font_size_larger"  design="Bold"/>
			<HBox>
				<VBox width="50%">
					<items>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Description"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/MATNR}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Model year"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/ZZMOYR}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Colour"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/Colour}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Driver"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/Driver}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Start Date"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/EstStartDt}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Est. Assoc Price"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/EstAssPrice}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Status"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/Status}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="SFX"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/ZZSUFFIX}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Zone"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/ZZZONE}" design="Bold"/>
							</items>
						</HBox>
					</items>
				</VBox>
				<VBox width="50%">
					<items>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Series"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/ZZSERIES}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="VIN"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/VHVIN}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Colour Code"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/ZZEXTCOL}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Driver Dept"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/QAUSP}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Est. End Date"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/EndDate}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="Est. Non Assoc. Price"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/NonAssPrice}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="SFX Options"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/Option}" design="Bold"/>
							</items>
						</HBox>
						<HBox class="sapUiTinyMargin">
							<items>
								<Label text="APX"/>
								<Label text=":" class="sapUiTinyMarginEnd"/>
								<Label text="{VehicleInfo>/ZZAPX}" design="Bold"/>
							</items>
						</HBox>

					</items>
				</VBox>
			</HBox>
			<Label text="Reservation Details" class="sapUiSmallMargin font_size_larger" design="Bold"/>
			<HBox>
				<VBox width="50%">
					<Text text="Requestor Information" class="sapUiTinyMargin"/>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Requestor Type"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Label text="{VehicleInfo>/ZZREQTYP}" design="Bold"/>
						</items>
					</HBox>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Requestor Name"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Label text="{VehicleInfo>/RequestorName}" design="Bold"/>
						</items>
					</HBox>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Request Date"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Label text="{VehicleInfo>/zzcretms}" design="Bold"/>
						</items>
					</HBox>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Requests Approved"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Label text="{VehicleInfo>/ReqAppr}" design="Bold"/>
						</items>
					</HBox>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Requests Pending Approval"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Label text="{VehicleInfo>/ReqPend}" design="Bold"/>
						</items>
					</HBox>
				</VBox>
				<VBox width="50%">
					<Text text="Reserved For Information" class="sapUiTinyMargin"/>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Reserved for Type"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Label text="{VehicleInfo>/ZZPURTYP}" design="Bold"/>
						</items>
					</HBox>
					<HBox class="sapUiTinyMargin">
						<items>
							<Label text="Reserved For Name"/>
							<Label text=":" class="sapUiTinyMarginEnd"/>
							<Label text="{VehicleInfo>/ZZPURNAME}" design="Bold"/>
						</items>
					</HBox>
				</VBox>
			</HBox>
		</content>
	<buttons>
	    <!--<Button xmlns="sap.m" text="Approve" id="btnAppr" press="onApprovePress"/>-->
	    <!--<Button xmlns="sap.m" text="Reject" id="btnRej"  press="onRejectPress"/>-->
	    <Button xmlns="sap.m" text="Cancel" id="btnCan" press="onCloseDialog"/>
	</buttons>
	</Dialog>
</core:FragmentDefinition>