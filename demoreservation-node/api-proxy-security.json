{
  "allowNoMatch": false,
  "services": [
    {
      "name": "Z_VEHICLE_DEMO_RESERVATION_SRV_02",
      "allowNoMatch": true,
      "resources": [
        {
          "path": "^reservationListSet(/\\$count)?(\\?.+)?$",
          "methods": ["GET"],
          "validations": [
            {
              "appliesToRoles": ["TCI_User"],
              "mode": "filterParam",
              "name": "Email",
              "value": "${userAttributes.Email}"
            }
          ]
        },
        {
          "path": "^VehicleDetailSet\\((.+)\\)(\\?.+)?$",
          "methods": ["GET"],
          "validations": [
            {
              "appliesToRoles": ["TCI_User"],
              "mode": "namedAndUnnamedParam",
              "regExpGroup": 1,
              "name": "Email",
              "value": "'${userAttributes.Email}'",
              "caseSensitive": false
            }
          ]
        },
        {
          "path": "^vehicleListSet?(\\?.+)?$",
          "methods": ["GET"],
          "validations": [
            {
              "appliesToRoles": ["TCI_User"],
              "mode": "filterParam",
              "name": "Emp",
              "value": "E"
            }
          ]
        }
      ]
    },
    {
      "name": "tci/internal/api/v1.0/security/ldap/rest",
      "allowNoMatch": false,
      "resources": [
        {
          "path": "^getUserByUID$",
          "methods": ["POST"]
        }
      ]
    }
  ]
}
